<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">

        <shape android:shape="rectangle">
            <stroke android:width="2dp"  android:color="#DF30E6BF"/>
            <corners android:radius="2dp" />
            <solid android:color="#5907F0BE"/>

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#591ABC9C">
            <ripple android:color="#591ABC9C">
                <stroke android:width="5dp" android:color="#591ABC9C"/>
            </ripple>
            </solid>
        </shape>
    </item>
</selector>
